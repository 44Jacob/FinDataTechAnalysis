   "source": [
    "# Objective Function (Negative Sharpe Ratio)\n",
    "def neg_sharpe_ratio(weights, expected_returns, cov_matrix, risk_free_rate=0.01):\n",
    "    p_var = np.dot(weights.T, np.dot(cov_matrix, weights))\n",
    "    p_ret = np.dot(weights, expected_returns)\n",
    "    return -(p_ret - risk_free_rate) / np.sqrt(p_var)"
   ]
  },
